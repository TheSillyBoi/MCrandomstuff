{
	"schemaVersion": 1,
	"id": "randomstuff",
	"version": "${version}",
	"name": "randomstuff",
	"description": "This is me trying to learn how to make a minecraft mod, and adding just about whatever I think of adding",
	"authors": [
		"Adrian Tennies!"
	],
	"contact": {
		"homepage": "https://adriantennies.vercel.app",
		"sources": "https://github.com/thesillyboi/MCrandomstuff"
	},
	"license": "MIT",
	"icon": "assets/randomstuff/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"adrian.randomstuff.Randomstuff"
		],
		"client": [
			"adrian.randomstuff.RandomstuffClient"
		]
	},
	"mixins": [
		"randomstuff.mixins.json",
		{
			"config": "randomstuff.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.16.14",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}